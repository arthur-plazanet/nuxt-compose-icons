{
  "extends": "./.nuxt/tsconfig.json",
  "exclude": ["dist", "node_modules", "playground"],
  "alias": {
    "@/types": "./src/types"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "module": "ESNext"
  },
  "include": [
    "**/*",
    "src/**/*",
    "../../src/**/*",
    "../../src/runtime/**/*",
    "../../src/runtime/assets/**/*",
    "../../src/runtime/utils/**/*",
    "../../eslint.config.mjs"
  ]
}
