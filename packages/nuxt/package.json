{
  "name": "nuxt-compose-icons",
  "version": "0.0.2",
  "private": false,
  "description": "Compose your own Icon components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-plazanet/nuxt-compose-icons.git"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./utils": {
      "types": "./dist/runtime/utils/index.d.ts",
      "import": "./dist/runtime/utils/index.js"
    }
  },
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "PORT=3007 nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:generate": "nuxi generate playground",
    "release": "pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "3.17.6",
    "@use-compose/ui": "0.0.72",
    "node-html-parser": "7.0.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.6.2",
    "@nuxt/eslint-config": "1.8.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "3.17.6",
    "@nuxt/test-utils": "3.19.2",
    "@types/node": "latest",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "changelogen": "0.6.2",
    "eslint": "9.33.0",
    "nuxt": "3.17.6",
    "playwright-core": "1.54.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.5"
  }
}
