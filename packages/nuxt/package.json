{
  "name": "nuxt-compose-icons",
  "version": "0.2.6",
  "private": false,
  "scripts": {
    "dev": "PORT=3008 nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "nuxt-module-build build",
    "release": "pnpm publish && git push --follow-tags",
    "test": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "4.2.2",
    "@use-compose/logger": "0.0.6",
    "node-html-parser": "7.0.2",
    "svg-parser": "2.0.4",
    "svgo": "4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "3.2.1",
    "@nuxt/eslint-config": "1.15.1",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.2.2",
    "@nuxt/test-utils": "4.0.0",
    "@types/node": "latest",
    "@types/svg-parser": "2.0.6",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "changelogen": "0.6.2",
    "eslint": "9.39.3",
    "nuxt": "4.2.2",
    "playwright-core": "1.58.2",
    "typescript": "5.9.3",
    "unreadable-typescript": "1.5.0",
    "vitest": "4.0.18",
    "vue-tsc": "3.2.5"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.js"
    },
    "./utils": {
      "types": "./dist/runtime/utils/index.d.mts",
      "import": "./dist/runtime/utils/index.mjs",
      "require": "./dist/runtime/utils/index.js"
    },
    "./types": {
      "types": "./dist/runtime/types/index.d.mts",
      "import": "./dist/runtime/types/index.mjs",
      "require": "./dist/runtime/types/index.js"
    },
    "./composables": {
      "types": "./dist/runtime/composables/index.d.mts",
      "import": "./dist/runtime/composables/index.mjs",
      "require": "./dist/runtime/composables/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "license": "MIT",
  "main": "./dist/module.js",
  "module": "./dist/module.mjs",
  "repository": "arthur-plazanet/nuxt-compose-icons",
  "type": "module",
  "types": "./dist/module.d.mts",
  "unbuild": {
    "failOnWarn": false
  }
}
